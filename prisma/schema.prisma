// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

// Looking for ways to speed up your queries, or scale easily with your serverless or edge functions?
// Try Prisma Accelerate: https://pris.ly/cli/accelerate-init

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id        String   @id @default(uuid())
  email     String   @unique
  password  String
  createdAt DateTime @default(now())
}

model WhyChoose {
  id          String   @id @default(uuid())
  img         String
  title       String
  description String
  createdAt   DateTime @default(now())
}

model Process {
  id        String   @id @default(uuid())
  title     String
  img       String
  createdAt DateTime @default(now())
}

model Course {
  id           String   @id @default(uuid())
  title        String
  img          String
  video        String
  timestart    String
  countclass   String
  countstudent String
  timelearn    String
  calendar     String
  createdAt    DateTime @default(now())
}

model Excellent {
  id        String   @id @default(uuid())
  title     String
  img       String
  createdAt DateTime @default(now())
}

model Evaluate {
  id            String   @id @default(uuid())
  img           String
  video         String
  title         String
  star          String
  parentstudent String
  achievements  String
  class         String
  createdAt     DateTime @default(now())
}

model Information {
  id          String   @id @default(uuid())
  img         String
  title       String
  description String
  createdAt   DateTime @default(now())
}

model Partner {
  id        String   @id @default(uuid())
  title     String
  img       String
  createdAt DateTime @default(now())
}

model Expert {
  id        String   @id @default(uuid())
  title     String
  img       String
  createdAt DateTime @default(now())
}

model Contact {
  id             String   @id @default(uuid())
  logo           String
  sublogo        String
  slogan         String
  namecompany    String
  addresscompany String
  namecenter     String
  addresscenter  String
  mail           String
  phone          String
  createdAt      DateTime @default(now())
}

model WhyChooseProgram {
  id          String   @id @default(uuid())
  img         String
  subimg      String
  title1      String
  title2      String
  title3      String
  description String
  createdAt   DateTime @default(now())
}

model Receive {
  id        String   @id @default(uuid())
  title     String
  createdAt DateTime @default(now())
}

model TargetThumbnail {
  id        String   @id @default(uuid())
  title     String
  img       String
  createdAt DateTime @default(now())
}

model Target {
  id        String   @id @default(uuid())
  title     String
  createdAt DateTime @default(now())
}

model InformationThumbnail {
  id        String   @id @default(uuid())
  title     String
  img       String
  createdAt DateTime @default(now())
}

model InformationProgram {
  id        String   @id @default(uuid())
  img       String
  title     String
  createdAt DateTime @default(now())
}

model Step {
  id          String   @id @default(uuid())
  img         String
  title       String
  subtitle    String
  description String
  createdAt   DateTime @default(now())
}

model Time {
  id        String   @id @default(uuid())
  timestart DateTime
  timeend   DateTime
  location  String
  img       String
  createdAt DateTime @default(now())
}

model ScheduleThumbnail {
  id        String   @id @default(uuid())
  title     String
  img       String
  createdAt DateTime @default(now())
}

model Schedule {
  id        String   @id @default(uuid())
  title     String
  createdAt DateTime @default(now())
}

model SocialNetwork {
  id        String   @id @default(uuid())
  zalo      String
  youtube   String
  tiktok    String
  facebook  String
  location  String
  createdAt DateTime @default(now())
}
